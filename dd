#include <iostream>
using namespace std;

int N;

int MakeOne(int n){
  int count =0 ;
  while(n != 1){
    if(n%3 == 0){
      n/=3;
    }else if(n%2 == 0){
      n/=2;
    }
    else{
      n -= 1;
    }
    ++count;
  }
  return count;
}

int main() {
  cin>>N;

  cout<<MakeOne(N)<<endl;

  return 0;
}
